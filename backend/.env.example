# Variables de entorno para Railway
PORT=3000
NODE_ENV=production

# Base de datos PostgreSQL (Railway configurará DATABASE_URL automáticamente)
# DATABASE_URL=postgresql://user:password@host:port/database

# Frontend URL para CORS (Railway configurará RAILWAY_STATIC_URL)
# FRONTEND_URL será configurada por Railway

# Configuración opcional
ENABLE_CORS=true
SERVE_STATIC=true
